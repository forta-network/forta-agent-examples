{
  "name": "private-agent",
  "displayName": "Private Agent",
  "version": "0.0.1",
  "description": "Forta Agent in Javascript that emits private findings",
  "repository": "https://github.com/forta-network/forta-bot-examples/tree/master/minimum-balance-py",
  "chainIds": [
    1
  ],
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec \"forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest",
    "keygen": "node generate-keys.js",
    "obfuscate": "javascript-obfuscator ./src --output ./obfuscated --config obfuscation-config.js"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "forta-agent": "^0.1.42",
    "openpgp": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "nodemon": "^3.0.1",
    "javascript-obfuscator": "^4.1.0"
  }
}
